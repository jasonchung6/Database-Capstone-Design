<h1>{{main_title}}</h1>
<table>
    <tr>
        <td>Id</td>
        <td>Name</td>
        <td>Email</td>
        <td>Phone_number</td>
        <td>Major</td>
    </tr>
    {{#each student_res}}
    <form method="post">
        <tr>
            <td>{{S_Id}}</td>
            <td style="display: none"><input type="text" name="id" value={{S_Id}}></td>
            <td><input type="text" name="name" value="{{S_Name}}"></td>
            <td><input type="text" name="email" value="{{S_Email}}"></td>
            <td><input type="text" name="phonenumber" value="{{S_PhoneNumber}}"></td>
            <td><input type="text" name="major" value="{{Major}}"></td>
            <td><input type="submit" value="Modify" formaction="/update/student"></td>
        </tr>
    </form>
    {{/each}}
</table>